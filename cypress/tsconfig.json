{
    "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts",
    "src/**/*.ts" // Added a common source folder for TypeScript files
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
